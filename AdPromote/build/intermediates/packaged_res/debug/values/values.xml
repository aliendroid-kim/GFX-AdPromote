    <style name="InterstitialStyle" parent="android:Theme.Dialog">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:windowBackground">@android:color/white</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">false</item>
        

    </style>
    <style name="RatingBarStyle" parent="Theme.AppCompat">
       <item name="colorControlNormal">@color/color_rating_light</item>
       <item name="colorControlActivated">@color/color_rating_dark</item>
   </style>
    <style name="seekBarStyle" parent="@android:style/Widget.SeekBar">
       <item name="android:indeterminateOnly">false</item>
       <item name="android:progressDrawable">@drawable/seekbar</item>
       <item name="android:thumb">@drawable/seek_oval_red</item>
       <item name="android:splitTrack">false</item>
       <item name="android:minHeight">5dip</item>
       <item name="android:maxHeight">4dip</item>
   </style>
    <declare-styleable name="BannerView">
        <attr format="string" name="banner_installTitle"/>
        <attr format="color" name="banner_installColor"/>
        <attr format="color" name="banner_contentColor"/>
        <attr format="color" name="banner_bodyColor"/>
    </declare-styleable>
    <declare-styleable name="BannerYoutube">
        <attr format="string" name="banner_yt_titleButton"/>
        <attr format="string" name="banner_yt_description"/>
        <attr format="color" name="banner_yt_titleColor"/>
        <attr format="color" name="banner_yt_buttonColor"/>
        <attr format="color" name="banner_yt_contentColor"/>
        <attr format="color" name="banner_yt_bodyColor"/>
        <attr format="color" name="banner_yt_IconsColor"/>


    </declare-styleable>
    <declare-styleable name="NativeView">
        <attr format="string" name="native_installTitle"/>
        <attr format="color" name="native_installColor"/>
        <attr format="color" name="native_contentColor"/>
        <attr format="color" name="native_bodyColor"/>
        <attr format="integer" name="native_installRadius"/>
    </declare-styleable>
    <declare-styleable name="NativeYoutube">
        <attr format="string" name="native_yt_titleButton"/>
        <attr format="color" name="native_yt_buttonColor"/>
        <attr format="color" name="native_yt_adColor"/>
        <attr format="color" name="native_yt_bodyColor"/>
        <attr format="boolean" name="native_yt_showPreview"/>
        <attr format="integer" name="native_yt_buttonRadius"/>

    </declare-styleable>
